{
    "name": "symfony-bundles/redis-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Redis Bundle",
    "keywords": ["redis", "symfony", "bundle"],
    "homepage": "https://github.com/symfony-bundles/redis-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Khaperets",
            "email": "khaperets@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SymfonyBundles\\RedisBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^7.2",
        "predis/predis": "~1.1",
        "symfony/framework-bundle": "^4.3||^5.0",
        "symfony/yaml": "^4.3||^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpunit/php-code-coverage": "^7.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "scripts": {
        "composer-validate": "composer validate",
        "test": "vendor/bin/phpunit",
        "check-style": "vendor/bin/phpcs",
        "fix-style": "vendor/bin/phpcbf"
    }
}
